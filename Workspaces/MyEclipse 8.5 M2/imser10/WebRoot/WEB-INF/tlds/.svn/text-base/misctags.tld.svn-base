<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE taglib PUBLIC "-//Sun Microsystems, Inc.//DTD JSP Tag Library 1.1//EN" "http://java.sun.com/j2ee/dtds/web-jsptaglibrary_1_1.dtd">
<taglib>
  <tlibversion>1.0</tlibversion>
  <jspversion>1.1</jspversion>
  <shortname>misctags</shortname>
  <uri>http://pct/misctags</uri>
  <tag>
    <name>limitFunctions</name>
    <tagclass>sg.com.stellarstudios.imser.web.tag.misc.LimitFunctionsTag</tagclass>
    <info>Limits the functions based on the authorization given to the user.</info>
    <attribute>
      <name>modules</name>
      <required>true</required>
      <rtexprvalue>false</rtexprvalue>
    </attribute>
  </tag>
  <tag>
    <name>printDecimal</name>
    <tagclass>sg.com.stellarstudios.imser.web.tag.misc.PrintDecimalTag</tagclass>
    <info>Prints decimal</info>
    <attribute>
      <name>pageVar</name>
      <required>true</required>
      <rtexprvalue>false</rtexprvalue>
    </attribute>
  </tag>
  <tag>
    <name>buildMaintainEntityControls</name>
    <tagclass>sg.com.stellarstudios.imser.web.tag.misc.BuildMaintainEntityControlsTag</tagclass>
    <info>Builds maintain entity controls</info>
    <attribute>
      <name>entity</name>
      <required>true</required>
      <rtexprvalue>false</rtexprvalue>
    </attribute>
    <attribute>
      <name>create</name>
      <required>false</required>
      <rtexprvalue>false</rtexprvalue>
    </attribute>
    <attribute>
      <name>search</name>
      <required>false</required>
      <rtexprvalue>false</rtexprvalue>
    </attribute>
  </tag>
  <tag>
    <name>buildSelectBoxControls</name>
    <tagclass>sg.com.stellarstudios.imser.web.tag.misc.BuildSelectBoxControlsTag</tagclass>
    <info>Builds select box controls</info>
    <attribute>
      <name>propertyName</name>
      <required>true</required>
      <rtexprvalue>false</rtexprvalue>
    </attribute>
    <attribute>
      <name>folder</name>
      <required>true</required>
      <rtexprvalue>false</rtexprvalue>
    </attribute>
    <attribute>
      <name>mapping</name>
      <required>true</required>
      <rtexprvalue>false</rtexprvalue>
    </attribute>
  </tag>
  <tag>
    <name>setActionExtension</name>
    <tagclass>sg.com.stellarstudios.imser.web.tag.misc.SetActionExtensionAsPageAttributeTag</tagclass>
    <info>Sets the action extension onto the page attribute.</info>
    <attribute>
      <name>pageVar</name>
      <required>true</required>
      <rtexprvalue>false</rtexprvalue>
    </attribute>
  </tag>
  <tag>
    <name>setActionCommand</name>
    <tagclass>sg.com.stellarstudios.imser.web.tag.misc.SetActionCommandAsPageAttributeTag</tagclass>
    <info>Sets the action command (eg: preloadCreate/preloadUpdate) onto the page attribute.</info>
    <attribute>
      <name>pageVar</name>
      <required>true</required>
      <rtexprvalue>false</rtexprvalue>
    </attribute>
  </tag>
  <tag>
    <name>setModel</name>
    <tagclass>sg.com.stellarstudios.imser.web.tag.misc.SetModelAsPageAttributeTag</tagclass>
    <info>Sets the model object onto the page attribute in preload methods.</info>
    <attribute>
      <name>pageVar</name>
      <required>true</required>
      <rtexprvalue>false</rtexprvalue>
    </attribute>
  </tag>
  <tag>
    <name>printDate</name>
    <tagclass>sg.com.stellarstudios.imser.web.tag.misc.PrintDateTag</tagclass>
    <info>Prints the property value (Calendar object) of the specified page var.</info>
    <attribute>
      <name>property</name>
      <required>true</required>
      <rtexprvalue>false</rtexprvalue>
    </attribute>
  </tag>
  <tag>
    <name>printDateTime</name>
    <tagclass>sg.com.stellarstudios.imser.web.tag.misc.PrintDateTimeTag</tagclass>
    <info>Prints the property value (Calendar object) of the specified page var.</info>
    <attribute>
      <name>property</name>
      <required>true</required>
      <rtexprvalue>false</rtexprvalue>
    </attribute>
  </tag>
  <tag>
    <name>convertDate</name>
    <tagclass>sg.com.stellarstudios.imser.web.tag.misc.ConvertDateToStringAsPageAttributeTag</tagclass>
    <info>Converts the property value (Calendar object) of the specified page var.</info>
    <attribute>
      <name>toPageVar</name>
      <required>true</required>
      <rtexprvalue>false</rtexprvalue>
    </attribute>
    <attribute>
      <name>fromPageVar</name>
      <required>true</required>
      <rtexprvalue>false</rtexprvalue>
    </attribute>
    <attribute>
      <name>fromPageVarProperty</name>
      <required>true</required>
      <rtexprvalue>false</rtexprvalue>
    </attribute>
  </tag>
  <tag>
    <name>printSortLabel</name>
    <tagclass>sg.com.stellarstudios.imser.web.tag.misc.PrintSortLabelTag</tagclass>
    <info>Displays the sort label after computing.</info>
    <attribute>
      <name>urlPageVar</name>
      <required>true</required>
      <rtexprvalue>false</rtexprvalue>
    </attribute>
    <attribute>
      <name>type</name>
      <required>true</required>
      <rtexprvalue>false</rtexprvalue>
    </attribute>
    <attribute>
      <name>label</name>
      <required>true</required>
      <rtexprvalue>false</rtexprvalue>
    </attribute>
    <attribute>
      <name>field</name>
      <required>true</required>
      <rtexprvalue>false</rtexprvalue>
    </attribute>
  </tag>
  <tag>
    <name>printMultiPage</name>
    <tagclass>sg.com.stellarstudios.imser.web.tag.misc.PrintMultiPageTag</tagclass>
    <info>Displays the multipage label.</info>
    <attribute>
      <name>urlPageVar</name>
      <required>true</required>
      <rtexprvalue>false</rtexprvalue>
    </attribute>
    <attribute>
      <name>resultsPerPagePageVar</name>
      <required>true</required>
      <rtexprvalue>false</rtexprvalue>
    </attribute>
    <attribute>
      <name>currentPagePageVar</name>
      <required>true</required>
      <rtexprvalue>false</rtexprvalue>
    </attribute>
    <attribute>
      <name>collectionPageVar</name>
      <required>true</required>
      <rtexprvalue>false</rtexprvalue>
    </attribute>
  </tag>
  <tag>
    <name>limitFunctions</name>
    <tagclass>sg.com.stellarstudios.imser.web.tag.misc.LimitFunctionsTag</tagclass>
    <info>Limits navibar functions.</info>
    <attribute>
      <name>allowed</name>
      <required>true</required>
      <rtexprvalue>false</rtexprvalue>
    </attribute>
  </tag>
  <tag>
    <name>printIfHaveValueInStringArray</name>
    <tagclass>sg.com.stellarstudios.imser.web.tag.misc.PrintIfHaveValueInStringArrayTag</tagclass>
    <info>Show if value is in property</info>
    <attribute>
      <name>pageVar</name>
      <required>true</required>
      <rtexprvalue>false</rtexprvalue>
    </attribute>
    <attribute>
      <name>property</name>
      <required>true</required>
      <rtexprvalue>false</rtexprvalue>
    </attribute>
    <attribute>
      <name>value</name>
      <required>true</required>
      <rtexprvalue>false</rtexprvalue>
    </attribute>
  </tag>
</taglib>
